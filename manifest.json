{
  "manifest_version": 2,
  "name": "LNMIIT Auto Login",
  "description": "Sign In automatically to the LNMIIT login page after opening.",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "content_scripts": [
    {
      "matches": ["*://172.22.2.6/connect/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "storage"]
}
